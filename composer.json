{
  "name": "compropago/php-sdk",
  "type": "library",
  "description": "PHP SDK for Compropago API",
  "version": "3.1.0.0",
  "keywords": [
    "pagos",
    "internet",
    "payment",
    "gateway"
  ],
  "homepage": "https://compropago.com/documentacion/librerias",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Eduardo Aguilar",
      "email": "eduardo.aguilar@compropago.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev":{
    "phpunit/phpunit": ">=4.8"
  },
  "autoload": {
    "psr-4": {
      "CompropagoSdk\\": "CompropagoSdk"
    }
  },
  "scripts" : {
    "unittest": "./vendor/bin/phpunit tests/Test.php",
    "reqtest": "./vendor/bin/phpunit tests/Request.php"
  }
}
